<?xml version="1.0" encoding="utf-8" ?>
<LocalizationKeys>
  <Administration>
    <Edit>
      <List>
        <LocalizedString key="add_entry">
          <Translation culture="en-US">Add new entry</Translation>
          <Translation culture="bg-BG">Добави нов</Translation>
        </LocalizedString>
      </List>
    </Edit>
    <Account>
      <Activities>
        <LocalizedString key="SendInvite">
          <Translation culture="en-US">Invite users</Translation>
          <Translation culture="bg-BG">Покани потребители</Translation>
        </LocalizedString>
        <LocalizedString key="UserSettings">
          <Translation culture="en-US">Settings</Translation>
          <Translation culture="bg-BG">Настройки</Translation>
        </LocalizedString>
        <LocalizedString key="ApplicationSettings">
          <Translation culture="en-US">Global settings</Translation>
          <Translation culture="bg-BG">Глобални настройки</Translation>
        </LocalizedString>
      </Activities>
      <PasswordReset>
        <LocalizedString key="TypeEmailForPasswordReset">
          <Translation culture="en-US">Enter your e-mail address here and we'll send you the instructions for resetting your password.</Translation>
          <Translation culture="bg-BG">Въведете си имейла тук и ще ви изпратим упътванията, за да си възстановите достъпа.</Translation>
        </LocalizedString>
        <LocalizedString key="UnknownEmailError">
          <Translation culture="en-US">We can't reset your password. You may have followed a wrong or mistyped link.</Translation>
          <Translation culture="bg-BG">Не можем да ви възстановим паролата. Възможно е да сте използвали грешен линк.</Translation>
        </LocalizedString>
      </PasswordReset>
    </Account>
    <UserManagement>
      <SendInvite>
        <LocalizedString key="invitation_sent">
          <Translation culture="en-US">Invitation has been sent successfully.</Translation>
          <Translation culture="bg-BG">Поканата беше изпратена успешно.</Translation>
        </LocalizedString>
        <LocalizedString key="roles">
          <Translation culture="en-US">Roles</Translation>
          <Translation culture="bg-BG">Роли</Translation>
        </LocalizedString>
      </SendInvite>
    </UserManagement>
    <Authentication>
      <Login>
        <LocalizedString key="PasswordResetEmailSent">
          <Translation culture="en-US">A password reset email has been sent. If the email is valid, you'll be able to reset your password.</Translation>
          <Translation culture="bg-BG">Беше изпратен имейл за възстановяване на парола. Ако имейлът е правилен, ще можете да си възстановите достъпа.</Translation>
        </LocalizedString>
        <LocalizedString key="authentication_failure">
          <Translation culture="en-US">User and/or password incorrect.</Translation>
          <Translation culture="bg-BG">Грешно потребителско име и/или парола.</Translation>
        </LocalizedString>
        <LocalizedString key="Forgot_password">
          <Translation culture="en-US">Forgot your password?</Translation>
          <Translation culture="bg-BG">Забравили сте си паролата?</Translation>
        </LocalizedString>
      </Login>
    </Authentication>
