          <Translation culture="bg-BG">Парола</Translation>
        </LocalizedString>
        <LocalizedString key="authentication_failure">
          <Translation culture="en-US">User and/or password incorrect.</Translation>
          <Translation culture="bg-BG">Грешно потребителско име и/или парола.</Translation>
        </LocalizedString>
        <LocalizedString key="Forgot_password">
          <Translation culture="en-US">Forgot your password?</Translation>
          <Translation culture="bg-BG">Забравили сте си паролата?</Translation>
        </LocalizedString>
      </Login>
