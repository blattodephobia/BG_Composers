        <LocalizedString key="UserNameInUse">
          <Translation culture="en-US">A user with that name already exists.</Translation>
          <Translation culture="bg-BG">Това потребителско име е заето.</Translation>
        </LocalizedString>
      </Register>
      <SendInvite>
        <LocalizedString key="invitation_sent">
          <Translation culture="en-US">Invitation has been sent successfully.</Translation>
          <Translation culture="bg-BG">Поканата беше изпратена успешно.</Translation>
        </LocalizedString>
        <LocalizedString key="roles">
          <Translation culture="en-US">Roles</Translation>
          <Translation culture="bg-BG">Роли</Translation>
        </LocalizedString>
        <LocalizedString key="UserExists">
          <Translation culture="en-US">A user with this email is already registered.</Translation>
          <Translation culture="bg-BG">Вече е регистриран потребител с този имейл.</Translation>          
        </LocalizedString>
      </SendInvite>
    </UserManagement>
    <Authentication>
      <Login>
        <LocalizedString key="LoginButton">
          <Translation culture="en-US">Login</Translation>
